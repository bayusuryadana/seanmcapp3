{
    "go.formatTool": "goimports",
    "go.lintTool": "staticcheck",
    "go.lintOnSave": "file",
    "gopls": {
        "ui.completion.usePlaceholders": true,
        "ui.documentation.enabled": true,
        "analyses": {
            "unusedparams": true,
            "shadow": true
        },
    },
}
